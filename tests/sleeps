TEST_DESCRIPTION="A basic sleep test."
source test_header.sh

function slptst() { echo Sleeping $1 seconds.; sleep $1; }
PRLL_NR_CPUS=1 prll slptst 5 1 3 2 4 | sort | diff -q - sleeps.dat
PRLL_NR_CPUS=3 prll slptst 5 1 3 2 4 | sort | diff -q - sleeps.dat
PRLL_NR_CPUS=10 prll slptst 5 1 3 2 4 | sort | diff -q - sleeps.dat